{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Enables default permissions for Braincells desktop app",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "core:event:default",
    "shell:allow-open",
    "dialog:default",
    "fs:default",
    "fs:allow-read",
    "fs:allow-write",
    "http:default",
    "updater:default"
  ]
}
